{
  "name": "yproximite/payum-payzen",
  "description": "Payzen gateway for Payum",
  "keywords": [
    "payum",
    "payment",
    "Payzen",
    "Systempay",
    "Sogecommerce"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "payum/core": "^1.7"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.65",
    "php-http/guzzle6-adapter": "^1.0",
    "php-http/message-factory": "^1.1",
    "phpstan/phpstan": "^1.12.13",
    "phpstan/phpstan-strict-rules": "^1.6.1",
    "phpunit/phpunit": "^9.6.22",
    "twig/twig": "^3.17"
  },
  "config": {
    "bin-dir": "bin",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Yproximite\\Payum\\Payzen\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Yproximite\\Payum\\Payzen\\Tests\\": "tests/"
    }
  },
  "archive": {
    "exclude": [
      "ansible",
      ".php-cs-fixer.dist.php",
      ".travis.yml",
      "Makefile",
      "phpstan.neon",
      "phpunit.xml",
      "Vagrantfile"
    ]
  }
}
